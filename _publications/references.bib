@article{Loulloudes2015,
abstract = {Cloud computing enables on-demand provisioning of computing resources to IT solutions, following a utility-based approach. Technology and standardization developments in traditional public utilities enable customers to seamlessly migrate across utility providers without being required to make changes to their home appliances. Dubbed as the fifth utility, cloud computing customers should have user-friendly tools and mechanisms at their disposal, which preserve application deployments across different resource providers. Here, the authors present current efforts to develop an open source Cloud Application Management Framework (CAMF) based on the Eclipse Rich Client Platform. This framework facilitates cloud application lifecycle management in a vendor-neutral approach. {\textcopyright} 1997-2012 IEEE.},
author = {Loulloudes, Nicholas and Sofokleous, Chrystalla and Trihinas, Demetris and Dikaiakos, Marios D. and Pallis, George},
doi = {10.1109/MIC.2015.60},
issn = {1089-7801},
journal = {IEEE Internet Computing},
keywords = {Internet/Web technologies,cloud computing,interoperability,vendor neutrality},
month = {may},
number = {3},
pages = {54--59},
title = {{Enabling Interoperable Cloud Application Management through an Open Source Ecosystem}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84930663379{\&}partnerID=40{\&}md5=15b9f59310226fa6bfc8e6762fb63db8},
volume = {19},
year = {2015}
}
@inproceedings{Antoniades2015,
abstract = {The Cloud Application Management Framework (CAMF) enables Cloud application developers to design, deploy and manage their applications through an intuitive blueprint design. In this paper we show how Cloud application developers can utilize CAMF in order to have portable applications that can be deployed on different IaaS with minimal effort. Towards this goal, we introduce the Cloud Application Requirement Language (CARL). CARL can be used for defining the application software and hardware requirements, information that is then included into the TOSCA description of the Cloud application, alongside the application blueprint. CAMF's Information Service utilizes both these artifacts to provide IaaS specific configurations that fulfill the user's requirements. {\textcopyright} 2015 IEEE.},
address = {Limassol, Cyprus},
author = {Antoniades, Demetris and Loulloudes, Nicholas and Foudoulis, Athanasios and Sophokleous, Chrystalla and Trihinas, Demetris and Pallis, George and Dikaiakos, Marios and Kornmayer, Harald},
booktitle = {Proceedings - 2015 IEEE/ACM 8th International Conference on Utility and Cloud Computing, UCC 2015},
doi = {10.1109/UCC.2015.56},
isbn = {9780769556970},
keywords = {cloud application management,portability},
month = {dec},
pages = {354--360},
title = {{Enabling Cloud Application Portability}},
year = {2015}
}
