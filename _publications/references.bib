@inproceedings{Loulloudes2010,
abstract = {VANETs have been envisioned as an infrastructure for deploying Vehicular Information Systems (VIS) that among others provide drivers with an up-to-date view on the prevailing traffic conditions. In this work we evaluate the benefits of caching vehicular information obtained from such VIS through VITP, a location-aware, application-layer communication protocol that we extend to support caching. We present an evaluation study of our approach conducting extensive simulation on large scale vehicular networks under different realistic urban traffic conditions Our results identify the critical parameters that affect information quality in VANETs as well as demonstrate the viability and effectiveness of the cache-enabled VITP.},
author = {Loulloudes, Nicholas and Pallis, George and Dikaiakos, Marios D.},
booktitle = {2010 Eleventh International Conference on Mobile Data Management},
doi = {10.1109/MDM.2010.31},
file = {:Users/nicholas/Documents/Mendeley Desktop/Loulloudes, Pallis, Dikaiakos/2010 Eleventh International Conference on Mobile Data Management/Loulloudes, Pallis, Dikaiakos - 2010 - On the Evaluation of Caching in Vehicular Information Systems.pdf:pdf},
isbn = {978-1-4244-7075-4},
keywords = {VANET,VITP,Vehicular Information Systems,cache,caching evaluation},
pages = {218--220},
publisher = {IEEE},
title = {{On the Evaluation of Caching in Vehicular Information Systems}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5489646},
year = {2010}
}
